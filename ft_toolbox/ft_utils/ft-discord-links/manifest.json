{
  "manifest_version": 3,
  "name": "42 Discord Profile Links",
  "version": "1.0",
  "description": "Adds intra profile links to Discord usernames",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://discord.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
